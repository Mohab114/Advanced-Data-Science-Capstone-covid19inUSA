15/845:
def get_rating(row):
    try:
        rating = row['rating']
    except:
        rating = None        
    return rating

def get_likes(row):
    try:
        likes = row['likes']['count']
    except:
        likes = None        
    return likes

def get_checkins(row):
    try:
        checkins = row['stats']['checkinsCount']
    except:
        checkins = None        
    return checkins

def get_price(row):
    try:
        price = row['price']['tier']
    except:
        price = None        
    return price

def get_tips(row):
    try:
        tips = row['tips']['count']
    except:
        tips = None        
    return tips
    
def get_venue_details(Venue_ID):
    venues_list=[]
    for venue_id in Venue_ID:
        print('. ')
            
        # create the API request URL
        url = 'https://api.foursquare.com/v2/venues/{}?client_id={}&client_secret={}&v={}'.format(venue_id, CLIENT_ID, CLIENT_SECRET, VERSION)
            
        # make the GET request
        results = requests.get(url).json()["response"]['venue']
        
        # return only relevant information for each nearby venue
        venues_list.append([(
            venue_id, 
            get_rating(results), 
            get_likes(results),
            get_checkins(results),
            get_price(results), 
            get_tips(results))])

    venues_details = pd.DataFrame([item for venue_list in venues_list for item in venue_list])
    venues_details.columns = ['Venue_ID', 
                             'Rating',
                             'Likes',
                             'CheckIns',
                             'Price',
                             'Tips']
                            
    
    return(venues_details)
