def get_venue_details(Venue_ID):
    venues_list=[]
    for venue_id in Venue_ID:
        print('. ')
            
        # create the API request URL
        url = 'https://api.foursquare.com/v2/venues/{}?client_id={}&client_secret={}&v={}'.format(venue_id, CLIENT_ID, CLIENT_SECRET, VERSION)
            
