for row in df.iloc[1:]:
    for i in range(new_df.shape[0]):
        if row['Postcode'] == new_df['Postcode']:
            new_df['Neighbourhood'] = new_df['Neighbourhood'] + ', ' + row['Neighbourhood'] 
        else:
            new_df.append(row)
