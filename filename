        for j in range(temppp.shape[0]):
            dis[j] = sqrt((temppp['Venue Latitude'].iloc[j] - temppp['Neighborhood Latitude'].iloc[j])**2 + (temppp['Venue Longitude'].iloc[j] - temppp['Neighborhood Longitude'].iloc[j])**2)
        temppp['dis'] = dis    
temppp
