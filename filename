for row in df.values[1:]:
    for i in range(new_df.shape[0]):
        if row[0] == new_df.Postcode.iloc[i]:
            new_df.Neighbourhood.iloc[i] = new_df.Neighbourhood.iloc[i] + ', ' + row[2] 
        else:
            new_df = new_df.append(row)
